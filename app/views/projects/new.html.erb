<h1>Projects#new</h1>
<p>Find me in app/views/projects/new.html.erb</p>

<div class = "create_project_form" >
  <%= simple_form_for @project do |f| %>
    <%= f.input :name %>
    <%= f.input :instructions %>
    <%= f.input :hook_size %>
    <%= f.input :yarn_size %>
    <%= f.input :difficulty %>
    <%= f.input :photo, as: :file %>
    <div class="submit_button">
    <%= f.button :submit %>
    </div>
  <% end %>
</div>

